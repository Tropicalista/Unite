<cfoutput>
    <!-- post -->
    <div class="post">

      <!-- content -->
      <div class="post-content">

          <a href="#cb.linkEntry(entry)#" title="#entry.getTitle()#">#entry.getTitle()#</a>
          <span class="date">#entry.getDisplayPublishedDate()#</span>


      </div><!-- end content -->
    </div><!-- end post -->

</cfoutput>